<launch>

    <arg name="rate" default="200" />

    <include file="$(find-pkg-share nuturtle_description)/launch/load_all.launch.xml">
        <arg name='color' value='red' />
        <arg name='use_rviz' value='false'/>
    </include>


    <node pkg='nusim' exec='nusim' name='nusim' output='screen'>
        <!-- <param from="$(find-pkg-share nusim)/config/basic_world.yaml"> -->
        <param name="rate" value="$(var rate)"/>
    </node>

</launch>